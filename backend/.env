# Server Configuration
PORT=3001
NODE_ENV=development

# CORS Configuration (if needed for production)
CORS_ORIGIN=http://localhost:5173

# Database Configuration (if you plan to add a database later)
# DATABASE_URL=your_database_url_here
# DB_HOST=localhost
# DB_PORT=5432
# DB_NAME=notes_manager
# DB_USER=your_username
# DB_PASSWORD=your_password

# JWT Configuration (if you plan to add authentication later)
# JWT_SECRET=your_jwt_secret_here
# JWT_EXPIRES_IN=24h

# AWS Configuration (if deploying to AWS)
# AWS_REGION=us-east-1
# AWS_ACCESS_KEY_ID=your_access_key
# AWS_SECRET_ACCESS_KEY=your_secret_key

# Logging Configuration
LOG_LEVEL=info

# Health Check Configuration
HEALTH_CHECK_PATH=/health
